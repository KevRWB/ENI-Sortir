{% extends 'base.html.twig' %}

{% block title %}Profil{% endblock %}

{% block h1 %}
    <h1></h1>
{% endblock %}

{% block content %}

    <h2>{{ user.pseudo }}</h2>
    <p>Nom : {{ user.firstName }} {{ user.lastName|upper }}</p>
    <p>Telephone :{{ user.phoneNumber }}</p>
    <p>Email : {{ user.email }}</p>
    <p>Campus :{{ user.campus.name }}</p>

   <a href="javascript:history.go(-1)"><button class="btn btn-lg btn-primary" >Retour</button></a>

{% endblock %}